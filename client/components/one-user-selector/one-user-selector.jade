div
  md-autocomplete(
  ng-show='items.length>0'
  md-selected-item="selectedItem"
  md-search-text="searchText"
  md-items="item in querySearch(searchText)"
  md-item-text="item | getName"
  md-floating-label='{{::label}}')
    md-item-template
      span(md-highlight-text="searchText") {{item | getName}}
